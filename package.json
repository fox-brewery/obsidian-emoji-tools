		"@types/react": "^18.2.55",
		"@types/react-dom": "^18.2.19",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"eslint": "^8.56.0",
		"prettier": "^3.2.5",
		"prettier-plugin-pkg": "^0.18.1",
		"rollup": "^4.10.0",
		"rollup-plugin-import-css": "^3.4.0",
		"rollup-plugin-postcss": "^4.0.2",
		"rollup-plugin-scss": "^4.0.0",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3"
